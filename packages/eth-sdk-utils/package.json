    "test": "jest --passWithNoTests",
    "test-watch": "jest --watchAll --passWithNoTests"
  },
  "dependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/node": "12.12.9",
    "@types/secp256k1": "^4.0.1",
    "bn.js": "^5.1.3",
    "eth-sig-util": "^3.0.0",
    "js-sha3": "0.8.0",
    "secp256k1": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tsutils": "3.17.1",
    "typescript": "^4.1.2"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
